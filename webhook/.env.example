# Webhook Server Environment Variables
# Copy to .env and fill in the values

# Supabase Service Role Key (from supabase/.env)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Evolution API Key (from evolution/.env)
EVOLUTION_API_KEY=your_evolution_api_key_here

# Webhook port (external)
WEBHOOK_PORT=3002

# Public URLs (accessible from doctor's phone browser)
# Dev: use your machine's IP on the local network (e.g. http://192.168.1.100:3000)
# Prod: use the actual domain (e.g. https://essencemedicalclinic.com)
PANEL_BASE_URL=http://localhost:3000
SUPABASE_PUBLIC_URL=http://localhost:8000
